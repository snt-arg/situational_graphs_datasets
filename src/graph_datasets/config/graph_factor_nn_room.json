{   
    "base_graphs": {
        "grid_dims" : [[150,150],[150,150]],
        "playground_size": [20, 20],
        "max_room_entry_size" : [60,60],
        "min_room_entry_size" : [15,15],
        "room_center_distances" : [0.1,0.1],
        "wall_thickness" : [0.05,0.15],
        "n_buildings" : 200
    },
    "noise":{
        "global":{
            "active": true,
            "translation": 1.0,
            "rotation": 1.0
        },
        "room": {
            "active": false,
            "translation": 0.01,
            "rotation": 0.004
        },
        "ws": {
            "active": false,
            "rotation": 0.015
        }
    },
    "multiview": {
        "number":2,
        "overlapping": 2
    },
    "training_split": {
        "val" : 0.1,
        "test" : 0.1
    },
    "postprocess": {
        "training": {
            "use_gt": true,
            "K_nearest_max": 0,
            "K_nearest_types" : [],
            "K_random_max": 0,
            "K_random_types" : [],
            "node_dropout": 0.0,
            "directed" : false,
            "room_merge_ratio" : 5.0
        },
        "final": {
            "use_gt": false,
            "K_nearest_max": 15,
            "K_random" : 0,
            "node_dropout": 0.5
        }
    },
    "initial_features": {
        "ws_node" : ["length", "normals"],
        "edge" : ["min_dist", "relative_pos"]
    }
}